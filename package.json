{
  "name": "eventstore-ts-client",
  "version": "0.2.0",
  "description": "Eventstore javascript client library written in typescript",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "./scipts/build.sh",
    "lint": "eslint --cache .",
    "lint:fix": "eslint . --fix",
    "test": "npm run test:unit && npm run test:integration",
    "test:coverage": "npm run test:coverageUnit && npm run test:coverageIntegration && npm run combine-coverage",
    "mocha:all": "mocha test/**/*.test.ts",
    "mocha:unit": "mocha test/unitTests/**/*.test.ts",
    "test:unit": "npm run mocha:unit",
    "test:coverageUnit": "nyc --temp-directory=./.nyc_unit --reporter=text npm run test:unit",
    "mocha:integration": "mocha test/integrationTests/**/*.test.ts",
    "test:integration": "./scipts/integrationTest.sh",
    "test:coverageIntegration": "nyc --temp-directory=./.nyc_integration --reporter=text npm run test:integration",
    "document": "typedoc --theme markdown --out typedoc",
    "combine-coverage": "./scipts/combineCoverage.sh && nyc report --reporter=json --reporter=text --reporter=lcov && mv ./coverage/coverage-final.json ./coverage/coverage.json",
    "allcoverage": "./scipts/allTest.sh"
  },
  "files": [
    "dist/**/*"
  ],
  "pre-push": [
    "lint"
  ],
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  },
  "engineStrict": true,
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@types/bunyan": "^1.8.6",
    "@types/long": "^4.0.0",
    "@types/node": "^11.13.4",
    "@types/uuid": "^3.4.4",
    "bunyan": "^1.8.12",
    "long": "^4.0.0",
    "protobufjs": "^6.8.8",
    "uuid4": "^1.1.4"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "@typescript-eslint/eslint-plugin": "^1.6.0",
    "@typescript-eslint/parser": "^1.6.0",
    "acorn": "^6.1.1",
    "chai": "^4.2.0",
    "esdoc": "^1.1.0",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-integrate-manual-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "esdoc-typescript-plugin": "^1.0.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-prettier": "^3.0.1",
    "git-pre-push": "0.0.5",
    "mocha": "^6.0.2",
    "nyc": "^13.3.0",
    "prettier": "^1.16.4",
    "source-map-support": "^0.5.12",
    "ts-node": "^8.0.3",
    "typedoc": "^0.14.2",
    "typedoc-plugin-markdown": "^1.1.27",
    "typescript": "^3.4.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sebastianwessel/eventstore-ts-client.git"
  },
  "keywords": [
    "eventstore",
    "client",
    "typescript"
  ],
  "author": "Sebastian Wessel",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/sebastianwessel/eventstore-ts-client/issues"
  },
  "homepage": "https://sebastianwessel.github.io/eventstore-ts-client/"
}
